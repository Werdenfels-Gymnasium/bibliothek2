import java.util.LinkedList;
import java.io.*;

public class main {

	public static void main(String[] args) {
		File file = new File ("erststart.dat");
		if (file!=null) {
			System.out.println("Achtung! Programmnerststart, Dateien k√∂nnten verloren gehen!");
			try {
				file.createNewFile();
			} catch (IOException e) {
				e.printStackTrace();
			}
		}
		else {
			System.out.println("Programm wird aus Speicher geladen, dies kann einige Zeit in Anspruch nehmen");
		}
	}

}
