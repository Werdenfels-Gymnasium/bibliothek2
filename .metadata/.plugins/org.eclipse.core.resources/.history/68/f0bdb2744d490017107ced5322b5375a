package model;

public class UnterMittelStufe{
	int jahrgangsstufe;
	int anzahl;
	char klasse;
	Lehrer lehrer;
	
	public UnterMittelStufe (int jahrgangsstufe, int anzahl, char klasse, Lehrer lehrer) {
		this.jahrgangsstufe = jahrgangsstufe;
		this.anzahl = anzahl;
		this.klasse = klasse;
		this.lehrer = lehrer;
	}
}
