package model;

public class UnterMittelSchueler {
	String vorname;
	String nachname;
	String gebDat;
	Klasse klasse;
	
	public UnterMittelSchueler (String vorname, String nachname, String geburtsDatum, Klasse klasse) {
		this.vorname = vorname;
		this.nachname = nachname;
		this.gebDat = geburtsDatum;
		this.klasse = klasse;
	}
